                    <label for="type">Type</label><br>
                    <input type="text" id="type" name="type" required><br>
                    <label for="eventId">EventID</label><br>
                    <input type="text" id="eventId" name="eventId" required><br>
                    <label for="event-value">EventValue</label><br>
                    <input type="text" id="event-value" name="event-value" required><br>
                    <label for="reward-value">RewardValue</label><br>
                    <input type="text" id="reward-value" name="reward-value" required><br>
                    <button class="add-btn">Add</button>
                </form>
            </div>
        </div>
    </div>
</div>

