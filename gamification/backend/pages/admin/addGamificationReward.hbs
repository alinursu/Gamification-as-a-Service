    <link rel="stylesheet" href="/styles/admin-add.css">

</head>
<body>
<div class="container">
    <div class="top-nav">
        <a href=""> ADMIN PANEL - Gamification Rewards</a>
    </div>
    <div class="box">
        <div class="side-nav">
            <a href="/admin/gamification-rewards" class="btn"> View All </a>
            <a href="" class="btn"> Add </a>
            <a href="/admin/gamification-rewards" class="btn">BACK</a>
        </div>
        <div class="content">
            <div class="form">
                <form method="post">
                    <label for="id">ID</label><br>
                    <input type="text" id="id" name="id" required><br>
                    <label for="api-key">SystemAPIKey</label><br>
                    <input type="text" id="api-key" name="api-key" required><br>
                    <label for="name">Name</label><br>
